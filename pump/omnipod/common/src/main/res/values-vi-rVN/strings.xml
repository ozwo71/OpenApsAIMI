    <!-- Omnipod - Pod Activation Wizard -->
    <string name="omnipod_common_pod_activation_wizard_start_pod_activation_title">Làm đầy Pod</string>
    <string name="omnipod_common_pod_activation_wizard_initialize_pod_title">Khởi tạo Pod</string>
    <string name="omnipod_common_pod_activation_wizard_attach_pod_title">Gắn Pod</string>
    <string name="omnipod_common_pod_activation_wizard_attach_pod_text">Chuẩn bị vị trí gắn. Tháo nắp kim và lớp keo bảo vệ của Pod, sau đó gắn Pod vào vị trí tiêm.\n\nNếu kim truyền (cannula) bị lòi ra, vui lòng nhấn <b>Hủy</b> và Loại bỏ Pod của bạn.\n\nNhấn <b>Tiếp theo</b> để đưa kim truyền vào và bắt đầu truyền basal.</string>
    <string name="omnipod_common_pod_activation_wizard_attach_pod_confirm_insert_cannula_text">Khi bạn nhấn <b>OK</b>, kim truyền sẽ được đưa vào. Hãy đảm bảo rằng bạn đã gắn Pod vào vị trí gắn.</string>
    <string name="omnipod_common_pod_activation_wizard_insert_cannula_title">Gắn kim truyền</string>
    <string name="omnipod_common_pod_activation_wizard_insert_cannula_text">Đang thiết lập lịch basal ban đầu và đưa kim truyền vào.\n\nKhi kim truyền đã được đưa vào thành công, bạn có thể nhấn <b>Tiếp theo</b>.</string>
    <string name="omnipod_common_pod_activation_wizard_pod_activated_title">Đã kích hoạt Pod</string>
    <string name="omnipod_common_pod_activation_wizard_pod_activated_text">Pod mới đã được kích hoạt.\n\nLịch basal của bạn đã được lập trình và kim truyền đã được đưa vào.\n\nVui lòng kiểm tra xem kim truyền đã được đưa vào đúng chưa và thay Pod nếu bạn cho rằng chưa đúng.</string>
    <!-- Omnipod - Pod Deactivation Wizard -->
    <string name="omnipod_common_pod_deactivation_wizard_start_pod_deactivation_title">Hủy kích hoạt Pod</string>
    <string name="omnipod_common_pod_deactivation_wizard_start_pod_deactivation_text">Nhấn <b>Tiếp theo</b> để hủy kích hoạt Pod.\n\n<b>Lưu ý:</b> Thao tác này sẽ tạm dừng toàn bộ việc truyền insulin và hủy kích hoạt Pod.</string>
    <string name="omnipod_common_pod_deactivation_wizard_deactivating_pod_title">Đang hủy kích hoạt Pod</string>
    <string name="omnipod_common_pod_deactivation_wizard_deactivating_pod_text">Đang hủy kích hoạt Pod.\n\nKhi quá trình hủy kích hoạt hoàn tất thành công, bạn có thể nhấn <b>Tiếp theo</b>.</string>
    <string name="omnipod_common_pod_deactivation_wizard_pod_deactivated_title">Đã hủy kích hoạt Pod</string>
    <string name="omnipod_common_pod_deactivation_wizard_pod_deactivated_text">Pod của bạn đã được hủy kích hoạt.\n\nVui lòng tháo Pod ra khỏi cơ thể và cho vào thùng rác.</string>
    <string name="omnipod_common_pod_deactivation_wizard_pod_discarded_title">Đã xóa Pod</string>
    <string name="omnipod_common_pod_deactivation_wizard_pod_discarded_text">Trạng thái Pod đã bị loại bỏ. Việc truyền insulin không bị tạm dừng vì Pod chưa được hủy kích hoạt đúng cách!\n\nVui lòng tháo Pod ra khỏi cơ thể và cho vào thùng rác.</string>
    <string name="omnipod_common_pod_deactivation_wizard_discard_pod_confirmation">Nếu bạn loại bỏ Pod, bạn sẽ không thể giao tiếp với nó nữa. Bạn chỉ nên thực hiện khi mọi liên lạc với Pod đều liên tục thất bại. Bạn có chắc chắn muốn loại bỏ Pod không?</string>
    <string name="omnipod_common_pod_deactivation_wizard_discard_pod">Xóa Pod</string>
